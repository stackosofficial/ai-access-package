{
  "name": "@decloudlabs/sky-ai-accesspoint",
  "version": "0.4.8",
  "description": "AI Access Point API with improved Supabase compatibility",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node lib/init.js",
    "dev": "nodemon --exec ts-node src/index.ts",
